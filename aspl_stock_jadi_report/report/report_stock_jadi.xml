                                                    <t t-value="get_product_sale_qty(o,product)" t-set="product_val"/>
                                                    <t t-set="ending_qty"
                                                    t-value="product_beg_qty + product_val['product_qty_in'] + product_val['product_qty_out'] +
                                                    product_val['product_qty_internal'] + product_val['product_qty_inspect'] + product_val['product_qty_mrp'] + product_val['product_qty_adjustment']
                                                    "/>
                                                    <t t-set="today_movment_qty"
