                                                                <span t-esc="abs(product_val['product_qty_out'])"/>
                                                                <t t-set="sum_product_qty_out" t-value="sum_product_qty_out + product_val['product_qty_out']"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((product_val['product_qty_internal']))"/>
                                                                <t t-set="sum_product_qty_internal" t-value="sum_product_qty_internal + product_val['product_qty_internal']"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((product_val['product_qty_inspect']))"/>
                                                                <t t-set="sum_product_qty_inspect" t-value="sum_product_qty_inspect + product_val['product_qty_inspect']"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((product_val['product_qty_mrp']))"/>
                                                                <t t-set="sum_product_qty_production" t-value="sum_product_qty_production + product_val['product_qty_mrp']"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((product_val['product_qty_adjustment']))"/>
                                                                <t t-set="sum_product_qty_adjustment" t-value="sum_product_qty_adjustment + product_val['product_qty_adjustment']"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((today_movment_qty + product_beg_qty))"/>
                                                            </td>
                                                        </tr>
                                                    </t>
                                                    <t t-if=" not o.with_zero and not o.is_today_movement and ending_qty != 0.00">
                                                        <tr>
                                                            <td><span t-esc="product.default_code"/></td>
                                                            <td><span t-esc="product.name"/></td>
                                                            <td><span t-esc="product.product_tmpl_id.kode"/></td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((product_beg_qty))"/>
                                                                <t t-set="sum_begning" t-value="sum_begning + product_beg_qty"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((product_val['product_qty_in']))"/>
                                                                <t t-set="sum_product_qty_in" t-value="sum_product_qty_in + product_val['product_qty_in']"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="abs(product_val['product_qty_out'])"/>
                                                                <t t-set="sum_product_qty_out" t-value="sum_product_qty_out + product_val['product_qty_out']"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((product_val['product_qty_internal']))"/>
                                                                <t t-set="sum_product_qty_internal" t-value="sum_product_qty_internal + product_val['product_qty_internal']"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((product_val['product_qty_inspect']))"/>
                                                                <t t-set="sum_product_qty_inspect" t-value="sum_product_qty_inspect + product_val['product_qty_inspect']"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((product_val['product_qty_mrp']))"/>
                                                                <t t-set="sum_product_qty_production" t-value="sum_product_qty_production + product_val['product_qty_mrp']"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((product_val['product_qty_adjustment']))"/>
                                                                <t t-set="sum_product_qty_adjustment" t-value="sum_product_qty_adjustment + product_val['product_qty_adjustment']"/>
                                                            </td>
                                                            <td>
                                                                <span t-esc="'{0:,.4f}'.format((product_beg_qty + product_val['product_qty_in'] + product_val['product_qty_out'] + 
                                                                        product_val['product_qty_internal'] + product_val['product_qty_inspect'] + product_val['product_qty_mrp'] + product_val['product_qty_adjustment']))"/>
                                                            </td>
                                                        </tr>
                                                    </t>
                                                </t>
                                            </t>
                                        </tbody>
                                        <tr>
                                            <th style="border-top: 2px solid black" colspan="3">Grand Total</th>
                                            <th style="border-top: 2px solid black"><span t-esc="'{0:,.4f}'.format((sum_begning))"/></th>
                                            <th style="border-top: 2px solid black"><span t-esc="'{0:,.4f}'.format((sum_product_qty_in))"/></th>
                                            <th style="border-top: 2px solid black"><span t-esc="abs(sum_product_qty_out)"/></th>
                                            <th style="border-top: 2px solid black"><span t-esc="'{0:,.4f}'.format((sum_product_qty_internal))"/></th>
                                            <th style="border-top: 2px solid black"><span t-esc="'{0:,.4f}'.format((sum_product_qty_inspect))"/></th>
                                            <th style="border-top: 2px solid black"><span t-esc="'{0:,.4f}'.format((sum_product_qty_production))"/></th>
                                            <th style="border-top: 2px solid black"><span t-esc="'{0:,.4f}'.format((sum_product_qty_adjustment))"/></th>
                                            <th style="border-top: 2px solid black">
                                                <span t-esc="'{0:,.4f}'.format((sum_begning + sum_product_qty_in + sum_product_qty_out + sum_product_qty_production 
                                                            + sum_product_qty_internal + sum_product_qty_inspect + sum_product_qty_adjustment))"/>
                                            </th>
                                        </tr>
                                    </table>
                                    <p style="page-break-before:always;"/>
                                </t>
                            </div>
                        </div>
                    </t>
                </t>
        </template>

        <template id="report_stock_jadi">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-set="lang" t-value="o.company_id.partner_id.lang"/>
                    <t t-call="aspl_stock_jadi_report.report_stock_jadi_doc" t-lang="lang"/>
                </t>
            </t>
        </template>

    </data>
</flectra>