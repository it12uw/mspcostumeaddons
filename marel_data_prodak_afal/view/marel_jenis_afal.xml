<?xml version="1.0" encoding="UTF-8"?>
<flectra>
    <data noupdate="0">
		<record id="view_marel_jenis_afal_tree" model="ir.ui.view">
			<field name="name">view_marel_jenis_afal_tree</field>
			<field name="model">marel.jenis.afal</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="Jenis Afal" editable="top">
					<field name="name"/>
				</tree>
			</field>
		</record>

		<record id="view_marel_jenis_afal_form" model="ir.ui.view">
			<field name="name">view_marel_jenis_afal_form</field>
			<field name="model">marel.jenis.afal</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<form string="Jenis Afal">
					<sheet>
						<div class="oe_title">
							<label string="Jenis Afal"/>
						</div>
						<group>
							<group>
								<field name="name"/>
							</group>
						</group>
					</sheet>
					<div class="oe_chatter">
						<field name="message_follower_ids" widget="mail_followers"/>
						<field name="activity_ids" widget="mail_activity"/>
						<field name="message_ids" widget="mail_thread"/>
					</div>
				</form>
			</field>
		</record>
	</data>
</flectra>